{

	public static void main(String[] args) {
	int []arr= {20,5,7,90,50};
	
	for(int i=0;i<arr.length;i++)
	{
		for(int j=0;j<arr.length-1-i;j++)
		{
			if(arr[j]>arr[j+1])
			{
				int tem=arr[j];
				arr[j]=arr[j+1];
				arr[j+1]=tem;
				
			}
		}
	}
	System.out.println("after sorting array is");
	for(int i=0;i<arr.length;i++)
	{
		System.out.print(arr[i] +" ");
	}
	
	}

}
