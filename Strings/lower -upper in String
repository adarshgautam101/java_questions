public class Main
{
	public static void main(String[] args) {
	String str="AdarshGautam";
	char ch[]=str.toCharArray();
	for(int i=0;i<ch.length;i++)
	{
	   if(ch[i]>='a' && ch[i]<='z')
	   {
	       ch[i]=(char)(ch[i]-32);
	   }
	  

	} 
	for(int i=0;i<ch.length;i++)
	{
	    System.out.print(ch[i]+" ");
	}
	}
}
