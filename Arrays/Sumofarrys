import java.util.*;
public class Main{
    
    public static void main(String[]args)
    {
        
        int total=0;
        Scanner sc = new Scanner(System.in);
        int arr[]= new int[5];
        
         for(int i=0;i<arr.length;i++)
        {
            arr[i]=sc.nextInt();
        }
        for(int i=0;i<arr.length;i++)
        {
            total=arr[i]+total;
        }
      System.out.println(total);
    
        
    }
}
